generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model CBMxED {
  CBM_id   Int
  ED_id    Int
  Priority Int?

  @@id([CBM_id, ED_id])
}

model Descriptions {
  id          Int     @id @unique(map: "id_UNIQUE") @default(autoincrement())
  type        String? @db.VarChar(45)
  name        String? @db.VarChar(200)
  description String? @db.VarChar(9999)
}

model LCPxCBM {
  LCP_id   Int
  CBM_id   Int
  Priority Int?

  @@id([LCP_id, CBM_id])
}

model LCPxED {
  LCP_id   Int
  ED_id    Int
  Priority Int?

  @@id([LCP_id, ED_id])
}
