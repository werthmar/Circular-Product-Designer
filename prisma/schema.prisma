generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model cbmxed {
  id       Int  @unique(map: "id_UNIQUE") @default(autoincrement())
  CBM_id   Int
  ED_id    Int
  Priority Int?
}

model cdp_definitions {
  id                    Int     @id @default(autoincrement())
  gestaltungsprinzipien String? @db.VarChar(400)
  definitionen          String? @db.VarChar(9999)
}

model descriptions {
  id          Int     @id
  type        String? @db.Text
  name        String? @db.Text
  description String? @db.Text
  cdp_title   String? @db.VarChar(300)
}

model edxcdp {
  id     Int  @id @unique(map: "id_UNIQUE") @default(autoincrement())
  ed_id  Int?
  cdp_id Int?
}

model lcpxcbm {
  id       Int  @id @unique(map: "id_UNIQUE") @default(autoincrement())
  LCP_id   Int?
  CBM_id   Int?
  Priority Int?
}

model lcpxed {
  id       Int  @id @unique(map: "id_UNIQUE") @default(autoincrement())
  LCP_id   Int?
  ED_id    Int?
  Priority Int?
}
